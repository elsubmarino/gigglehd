<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gigglehd.persistence.BoardIDMapper">
	<insert id="insert">
		insert into tbl_board_id(id,num,category) values(#{id},#{num},#{category})
	</insert>
	<select id="getOne" resultType="int">
		select count(*) from tbl_board_id where id=#{id} and num=#{num} and category=#{category} 
	</select>
</mapper>